                "version": "dev-master"
            },
            "type": "package"
        },
        {
            "package":             {
                "autoload":                 {
                    "psr-0":                     {
                        "Vespolina\\CartBundle": ""
                    }
                },
                "name": "vespolina/cart-bundle",
                "source":                 {
                    "reference": "master",
                    "type": "git",
                    "url": "git://github.com/iampersistent/VespolinaCartBundle.git"
                },
                "target-dir": "Vespolina/CartBundle",
                "version": "dev-master"
            },
            "type": "package"
