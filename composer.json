{
    "authors": [
        {
            "homepage": "http://vespolina-project.org",
            "name": "Vespolina Team"
        }
    ],
    "autoload": {
        "psr-0":         {
            "": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "description": "Vespolina sandbox.",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },
    "homepage": "http://vespolina-project.org",
    "keywords": [
"shop",
"ecommerce",
"cart",
"item"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "vespolina/vespolina-sandbox",
    "repositories": [
        {
            "package":             {
                "name": "twitter/bootstrap",
                "source":                 {
                    "reference": "master",
                    "type": "git",
                    "url": "https://github.com/twitter/bootstrap.git"
                },
                "version": "master"
            },
            "type": "package"
        },
        {
            "package":             {
                "autoload":                 {
                    "psr-0":                     {
                        "Vespolina": "lib/"
                    }
                },
                "name": "vespolina/core",
                "source":                 {
                    "reference": "master",
                    "type": "git",
                    "url": "git://github.com/iampersistent/VespolinaCore.git"
                },
                "version": "dev-master"
            },
            "type": "package"
        },
        {
            "package":             {
                "autoload":                 {
                    "psr-0":                     {
                        "Vespolina": "lib/"
                    }
                },
                "name": "vespolina/cart",
                "source":                 {
                    "reference": "master",
                    "type": "git",
