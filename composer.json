                "autoload":                 {
                    "psr-0":                     {
                        "Vespolina\\Symfony2Bundle": ""
                    }
                },
                "version": "dev-master",
                "name": "vespolina/symfony2-bundle",
                "source":                 {
                    "reference": "master",
                    "type": "git",
                    "url": "git://github.com/iampersistent/VespolinaSymfony2Bundle.git"
                },
                "target-dir": "Vespolina/Symfony2Bundle",
                "version": "dev-master"
            },
            "type": "package"
        },
        {
            "package":             {
                "autoload":                 {
                    "psr-0":                     {
                        "Vespolina": "lib/"
                    }
                },
                "name": "vespolina/taxonomy",
                "source":                 {
                    "reference": "master",
                    "type": "git",
                    "url": "git://github.com/iampersistent/VespolinaTaxonomy.git"
                },
                "version": "dev-master"
            },
            "type": "package"
        }
